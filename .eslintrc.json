{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "eol-last": 0,
        "indent": 0,
        "class-methods-use-this": 0,
        "no-unused-vars": 0,
        "no-undef": 0,
        "import/no-unresolved": 0,
        "space-before-blocks": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "no-empty-function": 0,
        "max-len": 0,
        "no-new": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "arrow-parens": 0,
        "function-paren-newline": 0,
        "prefer-destructuring": 0
    }
}
